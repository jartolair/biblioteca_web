<h1>Usuarios</h1>
<div id="userDiv">
	<%
	Usuario usuario=null;
	Object u=session.getAttribute("usuario");
	if (u==null){
		response.sendRedirect("../login.jsp");
	}else{
		usuario=(Usuario) u;
		if (!usuario.getRol().equals("admin")){
			response.sendRedirect("../login.jsp");
		}
	}
	%>
